Package: RSSL
Version: 0.9.7
Title: Implementations of Semi-Supervised Learning Approaches for Classification
Authors@R: c(person("Jesse", "Krijthe", role = c("aut", "cre"),
    email = "jkrijthe@gmail.com"))
Depends:
    R(>= 2.10.0)
Imports:
    methods,
    Rcpp,
    MASS,
    kernlab,
    quadprog,
    Matrix,
    dplyr,
    tidyr,
    ggplot2,
    reshape2,
